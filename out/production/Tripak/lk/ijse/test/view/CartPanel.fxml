<AnchorPane prefHeight="961.0" prefWidth="1570.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.test.controller.CartPanelConntroller">
